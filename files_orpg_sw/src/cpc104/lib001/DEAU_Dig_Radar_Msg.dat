#################################################################################
#
# DEAU Attribute Description File Representing the Digital Radar Data
# Message Described in the RDA/RPG ICD (message #1)
#
# This CS Format file represents the structures found in the basedata.h file.
# This file was partially produced using a text file dump of the Digital
# Radar Data message found in the RDA/RPG ICD in DOORS.  The remainder of the 
# data and the format were manually entered.
#
#################################################################################

RDA_basedata_header.time type = int;
			 range = [0, 86399999];
			 unit = msec;
			 accuracy = 2000;
			 description = Time: Zulu reference time at which \
				       radial data was collected;

RDA_basedata_header.date type = short;
			 range = [1, 65535];
			 unit = d;
			 accuracy = 1;
			 description = Modified Julian Date: Current Julian \
				       date - 2440586.5(1);

RDA_basedata_header.unamb_range type = short;
				range = [115, 511];
				unit = km;
				accuracy = 0.1;
				conversion = [0.1, 0];
				description = Unambiguous Range: Unambiguous \
					      range, interval size;

RDA_basedata_header.azimuth type = ushort;
			    range = [0, 359.956055];
			    unit = deg;
			    accuracy = 0.1;
			    conversion = <bam_to_deg>;
			    description = Azimuth Angle: Azimuth angle at which \
					  radial data was collected;

RDA_basedata_header.azi_num type = short;
			    range = [1, 400];
			    unit = ;
			    accuracy = 1;
			    description = Azimuth Number: Radial number within \
					  elevation cut;

RDA_basedata_header.status type = short;
			   range = {0,1,2,3,4,128,129,130,131,132};
			   description = Radial Status (e.g. first, last);

RDA_basedata_header.elevation type = ushort;
			      range = [-.5, 40];
			      unit = deg;
			      accuracy = 0.1;
			      conversion = <bam_to_deg>;
			      description = Elevation Angle: Elevation angle \
					    at which radial radar data was \
					    collected;

RDA_basedata_header.elev_num type = short;
			     range = [1, 25];
			     unit = ;
			     accuracy = 1;
			     description = Elevation Number: Elevation number \
					   within volume scan;

RDA_basedata_header.surv_range type = short;
			       range = [-32.768, +32.768];
			       unit = km;
			       accuracy = 0.05;
			       conversion = [0.001, 0];
			       description = Surveillanc Range: Range to center \
					     of first surveillance gate (BIN);

RDA_basedata_header.dop_range type = short;
			      range = [-32.768, +32.768];
			      unit = km;
			      accuracy = 0.05;
			      conversion = [0.001, 0];
			      description = Doppler Range: Range to center of \
					    first Doppler gate (BIN);

RDA_basedata_header.surv_bin_size type = short;
				  range = [0.25, 4];
				  unit = km;
				  accuracy = 0.05;
				  conversion = [0.001, 0];
				  description = Surveillance Range Sample: Size \
						of surveillance sample interval;

RDA_basedata_header.dop_bin_size type = short;
				 range = [0.25, 4];
				 unit = km;
				 accuracy = 0.05;
				 conversion = [0.001, 0];
				 description = Doppler Range Sample Interval: \
					       Size of Doppler Sample Interval;

RDA_basedata_header.n_surv_bins type = short;
				range = [0, 460];
				unit = ;
				accuracy = 1;
				description = Number of Surveillance Bins: \
					      Number of surveillance bins for \
					      current radial;

RDA_basedata_header.n_dop_bins type = short;
			       range = [0, 920];
			       unit = ;
			       accuracy = 1;
			       description = Number of Doppler Bins: Number of \
					     Doppler bins for current radial;

RDA_basedata_header.sector_num type = short;
			       range = [1, 3];
			       unit = ;
			       accuracy = 1;
			       description = Cut Sector Number: Sector Number \
					     within cut;

RDA_basedata_header.calib_const type = float;
				range = [-50.0, +50.0];
				unit = dB;
				accuracy = 1;
				description = Calibration Constant (SYSCAL): \
					      Scaling constant used by PS to \
					      calculate reflectivity;

RDA_basedata_header.ref_ptr type = short;
			    range = {0, 100};
			    accuracy = 1;
			    description = Surveillance Pointer: Pointer (byte \
					  offset) to first location of \
					  surveillance data in radial;

RDA_basedata_header.vel_ptr type = short;
			    range = [0, 560];
			    accuracy = 1;
			    description = Velocity Pointer: Pointer (byte \
					  offset) to first location of velocity \
					  data in radial message;

RDA_basedata_header.spw_ptr type = short;
			    range = [0, 1480];
			    accuracy = 1;
			    description = Spectral Width Pointer: Pointer (byte \
					  offset) to first location of spectral \
					  width data in radial message;

RDA_basedata_header.vel_resolution type = short;
				   range = {0, 2, 4};
				   description = Doppler Velocity Resolution: \
						 Indicates scaling used for the \
						 Doppler Velocity;

RDA_basedata_header.vcp_num type = short;
			    range = [1, 767];
			    accuracy = 1;
			    description = Volume Coverage Pattern Number: \
					  Identifies VCP number being used;

RDA_basedata_header.ref_data_playback type = short;
				      range = {100};
				      accuracy = 1;
				      description = Archive II Surveillance \
						    Pointer: Pointer to first \
						    location of surveillance \
						    data in radial, used for \
						    Archive II;

RDA_basedata_header.dop_data_playback type = short;
				      range = [100, 560];
				      accuracy = 1;
				      description = Archive II Velocity \
						    Pointer: Pointer to first \
						    location of velocity \
						    data in radial, used for \
						    Archive II;

RDA_basedata_header.sw_data_playback type = short;
				     range = [100, 1480];
				     accuracy = 1;
				     description = Archive II Spectral Width \
					           Pointer: Pointer to first \
						   location of spectral width \
						   data in radial, used for \
						   Archive II;

RDA_basedata_header.nyquist_vel type = short;
				range = [0.00, 327.67];
				unit = m/s;
				accuracy = 0.003;
				conversion = [0.01, 0];
				description = Nyquist Velocity;

RDA_basedata_header.atmos_atten type = short;
				range = [-.02, -.002];
				unit = dB/km;
				accuracy = 0.004;
				conversion = [0.001, 0];
				description = ATMOS: Atmospheric Attenuation \
					      Factor;

RDA_basedata_header.threshold_param type = short;
				    range = [0.0, 20.0];
				    unit = dB;
				    accuracy = 0.1;
				    conversion = [0.1, 0];
				    description = TOVER: Threshold parameter \
						  which specifies the minimum \
						  difference in echo power \
						  between two resolution cells \
						  for them not to be labeled \
						  overlayed;

RDA_basedata_header.spot_blank_flag type = short;
				    range = {0, 1, 2, 4, 6, 7};
				    description = Radial Spot Blanking Status: \
						  Spot blanking status for \
						  current radial, elevation cut \
						  and volume scan.;

