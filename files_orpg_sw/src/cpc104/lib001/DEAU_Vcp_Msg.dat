				          unit = dB;
				          accuracy = 1;
				          conversion = [0.125, 0];
				          description = Differential Reflectivity Threshold: Signal to \
                                                        noise ratio (SNR) threshold for differential \
                                                        reflectivity;

VCP_elevation_cut_data_t.diff_phase_thresh type = short;
