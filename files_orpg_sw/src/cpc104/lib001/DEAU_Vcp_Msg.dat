					 range = [0, 999];
					 accuracy = 1;
                                         description = Doppler PRF Pulse Count/Radial: Sector 2 \
                                                       Doppler Pulse Count/Radial;

VCP_elevation_cut_data_t.edge_angle3 type = short;
				     range = [0.000000, 359.956055];
				     unit = deg;
				     accuracy = 1;
				     conversion = <bam_to_deg>;
				     description = Edge Angle: Sector 3 Azimuth Clockwise Edge Angle \
                                                   (denotes start angle);

VCP_elevation_cut_data_t.dopp_prf_num3 type = short;
				       range = [0, 8];
