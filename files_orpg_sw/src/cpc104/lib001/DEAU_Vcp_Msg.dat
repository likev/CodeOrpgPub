				          accuracy = 1;
				          conversion = [0.125, 0];
				          description = Correlation Coefficient Threshold: Signal to \
                                                         noise ratio (SNR) threshold for correlation \
                                                         coefficient;

VCP_elevation_cut_data_t.edge_angle1 type = short;
				     range = [0.000000, 359.956055];
				     unit = deg;
				     accuracy = 1;
				     conversion = <bam_to_deg>;
				     description = Edge Angle: Sector 1 Azimuth Clockwise Edge Angle \
				     		   (denotes start angle);

VCP_elevation_cut_data_t.dopp_prf_num1 type = short;
