	$(CP) $(LIBRPG_COMMON_DIR)/rpg_itc.c .

rpg_outbuf.c: 
	$(CP) $(LIBRPG_COMMON_DIR)/rpg_outbuf.c .

rpg_prod_request.c: 
	$(CP) $(LIBRPG_COMMON_DIR)/rpg_prod_request.c .

rpg_prod_support.c: 
	$(CP) $(LIBRPG_COMMON_DIR)/rpg_prod_support.c .

rpg_prod_compress.c: 
	$(CP) $(LIBRPG_COMMON_DIR)/rpg_prod_compress.c .

rpg_legacy_prod.c: 
	$(CP) $(LIBRPG_COMMON_DIR)/rpg_legacy_prod.c .

rpg_scan_sum.c: 
	$(CP) $(LIBRPG_COMMON_DIR)/rpg_scan_sum.c .

